# Copy this file to .env and set values as appropriate for your environment.

# Supabase (required for Save to Supabase feature)
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
REACT_APP_SUPABASE_KEY=YOUR_PUBLIC_ANON_KEY
# Used for Supabase email OTP redirect (optional if not using magic links)
REACT_APP_SITE_URL=http://localhost:3000

# AI generation (optional; used if no /api/ai/sow backend is available)
REACT_APP_OPENAI_API_KEY=sk-...
# Optional override; defaults to gpt-4o-mini if not set
REACT_APP_OPENAI_MODEL=gpt-4o-mini

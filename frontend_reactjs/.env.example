# Frontend environment variables
# Note: Do not commit real secrets.

# Supabase (optional if you use Supabase features)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=
# Used for magic link redirect (if using Supabase email auth)
REACT_APP_SITE_URL=http://localhost:3000

# Backend base URL (optional)
# If omitted, the app will call /api/* directly, which requires a dev proxy in package.json like:
#   "proxy": "http://localhost:8080"
# Otherwise set this explicitly to the backend_express host:
#   REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_BACKEND_URL=
